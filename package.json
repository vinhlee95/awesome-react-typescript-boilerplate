{
	"name": "awesome-react-typescript-boilerplate",
	"version": "1.0.0",
	"description": "",
	"main": "index.js",
	"scripts": {
		"start": "webpack-dev-server --config webpack/webpack.dev.js",
		"build": "webpack --config webpack/webpack.prod.js",
		"build:dev": "webpack --config webpack/webpack.dev.js",
		"tslint-prettier": "tslint-config-prettier-check ./tslint.json",
		"analyze:prod": "npm run build -- --env.presets analyze",
		"analyze:dev": "npm run build:dev -- --env.presets analyze",
		"lint": "tslint --project tsconfig.json",
		"fix-lint": "tslint --fix --project tsconfig.json",
		"check-types": "tsc",
		"format": "prettier \"**/*.+(js|jsx|json|ts|tsx|css|scss|md|mdx|graphql)\" --write"
	},
	"husky": {
		"hooks": {
			"pre-commit": "lint-staged & npm run check-types"
		}
	},
	"lint-staged": {
		"src/*.{ts, tsx}": "npm run lint",
		"**/*.+(js|jsx|json|ts|tsx|css|scss|md|mdx|graphql)": [
			"npm run format",
			"git add"
		]
	},
	"keywords": [],
	"author": "",
	"license": "ISC",
	"devDependencies": {
		"@babel/core": "7.4.3",
		"@babel/plugin-proposal-class-properties": "7.4.0",
		"@babel/plugin-proposal-object-rest-spread": "7.4.3",
		"@babel/plugin-syntax-dynamic-import": "7.2.0",
		"@babel/plugin-transform-react-constant-elements": "7.2.0",
		"@babel/plugin-transform-react-inline-elements": "7.2.0",
		"@babel/preset-env": "7.4.3",
		"@babel/preset-react": "7.0.0",
		"@babel/preset-typescript": "7.3.3",
		"@types/i18next-xhr-backend": "1.4.2",
		"@types/node": "11.13.2",
		"@types/react": "16.8.10",
		"@types/react-dom": "16.8.3",
		"@types/react-hot-loader": "4.1.0",
		"@types/react-redux": "7.0.6",
		"@types/react-router-dom": "4.3.1",
		"@types/redux-logger": "3.0.7",
		"babel-loader": "8.0.5",
		"babel-plugin-lodash": "3.3.4",
		"babel-plugin-transform-react-remove-prop-types": "0.4.24",
		"clean-webpack-plugin": "2.0.1",
		"copy-webpack-plugin": "5.0.2",
		"css-loader": "2.1.1",
		"dotenv-webpack": "1.7.0",
		"fork-ts-checker-webpack-plugin": "1.0.1",
		"html-webpack-plugin": "3.2.0",
		"husky": "1.3.1",
		"lint-staged": "8.1.5",
		"mini-css-extract-plugin": "0.5.0",
		"node-sass": "4.11.0",
		"optimize-css-assets-webpack-plugin": "5.0.1",
		"postcss-loader": "3.0.0",
		"postcss-preset-env": "6.6.0",
		"prettier": "1.16.4",
		"redux-logger": "3.0.6",
		"sass-loader": "7.1.0",
		"style-loader": "0.23.1",
		"tslint": "5.15.0",
		"tslint-config-prettier": "1.18.0",
		"tslint-eslint-rules": "5.4.0",
		"tslint-plugin-prettier": "2.0.1",
		"tslint-react": "4.0.0",
		"typescript": "3.4.1",
		"webpack": "4.29.6",
		"webpack-bundle-analyzer": "3.3.2",
		"webpack-cli": "3.3.0",
		"webpack-dev-server": "3.2.1",
		"webpack-merge": "4.2.1"
	},
	"dependencies": {
		"axios": "0.18.0",
		"connected-react-router": "6.3.2",
		"i18next": "15.0.9",
		"i18next-browser-languagedetector": "3.0.1",
		"i18next-xhr-backend": "2.0.1",
		"immer": "2.1.5",
		"query-string": "6.4.2",
		"react": "16.8.6",
		"react-dom": "16.8.6",
		"react-hot-loader": "4.8.3",
		"react-i18next": "10.7.0",
		"react-redux": "6.0.1",
		"react-router-dom": "5.0.0",
		"redux": "4.0.1",
		"redux-thunk": "2.3.0"
	},
	"browserslist": {
		"development": [
			"last 2 chrome versions",
			"last 2 firefox versions",
			"last 2 edge versions"
		],
		"production": [
			">0.2%",
			"last 4 versions",
			"Firefox ESR",
			"not ie < 11"
		]
	}
}
